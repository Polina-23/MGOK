#include <conio.h>
#include <stdlib.h>
#include <iostream>
using namespace std;
int main()
{
int a[9][9];
int i,j;
int range = 5;
for (i=0; i<10; i++)
{
    for(j=0; j<10; j++)
    {
         a[i][j]=rand() % range+1;
}
}
cout << "массив: "<<endl;
for (i=0; i<10; i++)
{
    for(j=0; j<10; j++)
    {
         cout << a[i][j] << "";
}
cout << "" <<endl;
}
getch();
}
